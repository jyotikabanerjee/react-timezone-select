{
  "name": "@jyotikabanerjee/react-timezone-select",
  "version": "0.1.0",
  "description": "Usable, dynamic antd style React Timezone Select",
  "scripts": {
    "start": "cd example && npm start",
    "prepublish": "npm run build",
    "postpublish": "npm run build:example && npm run deploy",
    "build": "tsc --project ./tsconfig.json",
    "build:example": "cd example && npm run build",
    "deploy": "gh-pages -d example/build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci "
  },
  "author": "Jyotika Banerjee",
  "homepage": "https://github.com/ndom91/react-timezone-select",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndom91/react-timezone-select.git"
  },
  "bugs": {
    "url": "https://github.com/ndom91/react-timezone-select/issues"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "timezone",
    "select",
    "react-select"
  ],
  "files": [
    "dist/*"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "react": "^17.0.1 || ^16",
    "react-dom": "^17.0.1 || ^16"
  },
  "dependencies": {
    "react-select": "^5.1.0",
    "spacetime": "^6.16.3",
    "timezone-soft": "^1.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "gh-pages": "^3.2.3",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}